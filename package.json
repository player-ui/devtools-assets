{
  "name": "vite-storybook-library",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "release": "auto shipit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "deploy": "pnpm run build-storybook && gh-pages -d storybook-static"
  },
  "dependencies": {
    "@devtools-ds/console": "^1.2.1",
    "@devtools-ds/icon": "^1.2.1",
    "@devtools-ds/navigation": "^1.2.1",
    "@devtools-ds/object-inspector": "^1.2.1",
    "@devtools-ds/object-parser": "^1.2.1",
    "@player-tools/devtools-common": "^0.4.1",
    "@player-tools/dsl": "^0.4.1",
    "@player-ui/asset-provider-plugin-react": "^0.6.0",
    "@player-ui/asset-transform-plugin": "^0.7.0",
    "@player-ui/partial-match-registry": "^0.7.0",
    "@player-ui/player": "^0.7.0",
    "@player-ui/react": "^0.5.1",
    "@player-ui/reference-assets-plugin": "^0.7.0",
    "@player-ui/reference-assets-plugin-react": "^0.7.0",
    "@player-ui/storybook": "^0.7.0",
    "@player-ui/types": "^0.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-reconciler": "^2.0.0",
    "react-split": "^2.0.14",
    "vitest": "^1.3.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.71",
    "@types/react-dom": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "auto": "^11.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "storybook": "^7.6.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vitest": "^1.3.1"
  },
  "repository": "player-ui/devtools-assets",
  "author": "test <test@test.com>",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ],
    "onlyPublishWithReleaseLabel": true
  }
}
